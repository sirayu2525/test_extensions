{
  "manifest_version": 3,
  "name": "My Extension",
  "version": "1.0",
  "description": "My first extension",
  "permissions": ["activeTab", "storage"],
  "action": {
      "default_popup": "popup.html"
  },
  "content_scripts": [
      {
          "matches": ["<all_urls>"],
          "js": ["content.js"]
      }
  ]
}